<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.ayvytr.easykotlinproject"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ayvytr.easykotlinproject.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.ayvytr.easykotlinproject.ResourcesActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="true">
        </activity>
        <activity android:name="com.ayvytr.easykotlinproject.LogTestActivity">
        </activity>
        <activity android:name="com.ayvytr.easykotlinproject.ext.StatusBarActivity">
        </activity>
    </application>

</manifest>